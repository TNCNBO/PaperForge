# MCP Agent Secrets Configuration Template
# Copy this file to mcp_agent.secrets.yaml and fill in your API keys

openai:
  api_key: "YOUR_OPENAI_API_KEY_HERE"
  base_url: "https://api.openai.com/v1"  # Or use SiliconFlow: https://api.siliconflow.cn/v1

anthropic:
  api_key: "YOUR_ANTHROPIC_API_KEY_HERE"

google:
  api_key: "YOUR_GOOGLE_API_KEY_HERE"

# Optional: Search API keys
brave:
  api_key: "YOUR_BRAVE_SEARCH_API_KEY_HERE"

bocha:
  api_key: "YOUR_BOCHA_API_KEY_HERE"
